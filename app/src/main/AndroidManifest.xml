<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pro.swati.ekart">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.pro.swati.ekart.OpenScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pro.swati.ekart.newUser"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.pro.swati.ekart.MainAppPage"
            android:label="@string/title_activity_main_app_page"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.pro.swati.ekart.IndividualProduct"
            android:label="@string/title_activity_individual_product"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.pro.swati.ekart.ShoppingCartWindow" />
        <activity android:name="com.pro.swati.ekart.CheckOutScreen" />
        <activity android:name="com.pro.swati.ekart.forgotPassword" />
        <activity android:name="com.pro.swati.ekart.IndividualProductSeller" />
        <activity android:name="com.pro.swati.ekart.AddProductForm" />
    </application>

</manifest>